
/* 头部step显示*/
	.formBuilder_step {
	height: 50px;
	padding: 0 10%;
	background: #E4EAEB;
	border-bottom: 1px solid #D0D0D0;
	box-shadow: 0 1px 0 #ffffff;
}
/* webhook start*/
	.formBuilder_web_hook_set {
	display: none;
	color: #336699;
	cursor: pointer;
	margin:5px 0px 0px 10px;
}
.formBuilder_edit_filed:hover .formBuilder_web_hook_set {
	display: block;
}
.setting_web_hook_form {
	margin: 15px;
}
.setting_web_hook_form .input {
	width: 65%;
	padding:10px;
}
.setting_web_hook span {
	color: #000000;
	margin-left:7px;
}
.setting_web_hook_form .web_hook_submit {
	width: 20%;
	line-height: 38px;
	vertical-align: top;
}
.web_hook_confirm {
	opacity:0;
	margin:3px 0px;
}
.setting_web_hook_help {
	margin-top: 16px;
	margin-left: 6px;
	cursor: pointer;
	display: inline-block;
	width: 20px;
	height: 20px;
	background-image: url(../images/icon/wikiHelp_big.png);
	background-repeat: no-repeat;
}
.setting_web_hook_help:hover {
	background-image: url(../images/icon/wikiHelp_big_hover.png);
}
.web_hook_list {
	height: auto;
	overflow:hidden
}
.web_hook_list li {
	padding:3px 0px;
	font-size:14px;
}
.web_hook_list li .del_webhook {
	color:#2776A4;
	display: none;
	float:right;
	font-size:12px;
	cursor:pointer;
}
.web_hook_list li:hover .del_webhook {
	display: inline;
}
.added_hook_tip {
	display: none;
	font-size:14px;
	color:#666;
	border-bottom:solid 2px rgb(89, 159, 209);
	margin:3px 0px;
	margin-top:8px;
	line-height:20px;
}
.added_hook_tip .save_tip {
	float:right;
	font-size:12px;
}
/* webhook end*/
.formBuilder_title {
	width: 18%;
	line-height: 50px;
	text-align: center;
	font-size: 18px;
	color: #999999;
	text-shadow: 0 1px 0 #fff;
	padding-left: 13px;
	float: left;
	border-radius: 6px 6px 0 0;
}
.formBuilder_title_active {
	color: #2776A4;
}
.formBuilder_title span {
	display: inline-block;
	width: 30px;
	color: #ffffff;
	margin-right: 10px;
	background: url(../images/icon/formBuilderStepGray30x30.png) 0 center no-repeat;
}
.formBuilder_title_active span {
	background: url(../images/icon/formBuilderStepBlue30x30.png) 0 center no-repeat;
}
.formBuilder_line {
	width: 20%;
	height: 50px;
	background: url(../images/icon/formBuilderStepLineGray.png) 0 center repeat-x;
	float: left;
}
.formBuilder_show {
	position: absolute;
	top: 51px;
	bottom: 0;
	width: 100%;
	/*overflow: hidden;*/
}
.formBuilder_interim {
	height: 44px;
	background: #F5F5F5;
	border-bottom: 1px solid #D0D0D0;
}
/* *****表单示例******/
	.formBuilder_example_all .formBuilder_interim {
	width: 620px;
}
.formBuilder_example_all {
	position: absolute;
	top: 0;
	/* left: 0;*/
	right: 0;
	bottom: 0;
	width: 600px;
	perspective: 1800px;
	-webkit-perspective: 1800px;
	-moz-perspective: 1800px;
	-ms-perspective: 1800px;
	-o-perspective: 1800px;
}
.formBuilder_step2_show .formBuilder_example_all {
	left: 0;
}
.formBuilder_example {
	position: absolute;
	top: 10px;
	left: 19px;
	/*left: -1px;
	修复1px差*/
	right: 20px;
	bottom: 50px;
	width: 580px;
	background: #FFFFFF;
	border: 1px solid #C1C1C1;
