	display: block;
}
.form_edit_schemefield {
	display: none;
	width: 0;
}
.form_edit_titlefield {
	width: auto;
	right: 0;
}
.formBuilder_interim_color {
	display: none;
	width: 0;
}
.formBuilder_edit {
	width: 100%;
}
.formBuilder_step2 .formBuilder_color_outer {
	position: absolute;
	top: 45px;
	left: 0;
	bottom: 0;
	padding-top: 8px;
	padding-bottom: 10px;
	width: 100%;
	border-right: 1px solid #D0D0D0;
	width: 100%;
	background-color: #fff;
}
.color_hide {
	display: none;
}
div.formBuilder_color_items {
	width: 29%;
}
div.color_show_block {
	padding: 8px 10% 0 10%;
}
/*.formBuilder_color_outer {
	display: none;
	width: 0;
}*/
}
/* 高分辨率下避免配色方案宽度太大*/
	@media screen and (min-width:1444px) {
	.formBuilder_edit {
	width: 52%;
}
.form_edit_schemefield {
	left: 52%;
}
.form_edit_titlefield {
	width: 52%;
}
.formBuilder_step2 div.formBuilder_color_outer {
	left: 52%;
}
p.formBuilder_interim_color {
	width: 45%;
}
