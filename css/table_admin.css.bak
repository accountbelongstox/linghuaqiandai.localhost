.pv_st{
	margin-right:8px;
}
.pv_st:before{    
	font-family: FontAwesome;
    font-weight: normal;
    font-style: normal;
    text-decoration: inherit;
    -webkit-font-smoothing: antialiased;
    display: inline;
    width: auto;
    height: auto;
    line-height: normal;
    vertical-align: baseline;
    background-image: none;
    background-position: 0 0;
    background-repeat: repeat;
    margin-top: 0;
    content: "\f0a6";
    text-decoration: inherit;
    display: inline-block;
    speak: none;
    padding-right:3px;
}
